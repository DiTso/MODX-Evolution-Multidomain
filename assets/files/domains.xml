<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<domains>
    <domain>
        <name>www.domain1.de</name>               <!-- Domain name -->
        <protocol>http://</protocol>              <!-- Protocol -->
        <site_id>1</site_id>                      <!-- ID of the domain resource -->
        <site_start>1</site_start>                <!-- ID of the start page -->
        <error_page>4</error_page>                <!-- ID of the 404 error page -->
        <offline_page>7</offline_page>            <!-- ID of the offline page -->
        <unauthorized_page>5</unauthorized_page>  <!-- ID of the unauthorized page --> 
        <keyword>dom</keyword>                    <!-- Keyword that is saved to the placeholder [+domainkey+] -->
    </domain>
    <domain>
        <name>sub.domain1.de</name>
        <protocol>http://</protocol>
        <site_id>2</site_id>
        <site_start>2</site_start>
        <error_page>4</error_page>
        <offline_page>8</offline_page>
        <unauthorized_page>5</unauthorized_page>
        <keyword>subdom</keyword>
    </domain>
    <domain>
        <name>intern.domain2.de</name>
        <protocol>http://</protocol>
        <site_id>3</site_id>
        <site_start>3</site_start>
        <error_page>4</error_page>
        <offline_page>9</offline_page>
        <unauthorized_page>5</unauthorized_page>
        <keyword>dom2</keyword>
    </domain>
</domains>
